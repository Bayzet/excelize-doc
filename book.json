{
    "title": "Excelize",
    "author": "xuri",
    "language": "en",
    "plugins": [
        "-sharing",
        "-lunr",
        "-search",
        "search-plus",
        "scripts",
        "sitemap-general",
        "language-picker",
        "favicon"
    ],
    "pluginsConfig": {
        "favicon": {
            "shortcut": "https://xuri.me/excelize/images/favicon.ico",
            "bookmark": "https://xuri.me/excelize/images/favicon.ico",
            "appleTouch": "./images/logo_trans_300x300.png",
            "appleTouchMore": {
                "120x120": "https://xuri.me/excelize/images/apple-touch-icon-120x120.png",
                "180x180": "https://xuri.me/excelize/images/apple-touch-icon-180x180.png"
            }
        },
        "sitemap-general": {
            "prefix": "https://xuri.me/excelize/"
        },
        "scripts": {
            "files": []
        }
    },
    "pdf": {
        "fontSize": 10,
        "pageNumbers": false,
        "paperSize": "a4",
        "margin": {
            "right": 0,
            "left": 0,
            "top": 0,
            "bottom": 0
        },
        "headerTemplate": null,
        "footerTemplate": null
    }
}